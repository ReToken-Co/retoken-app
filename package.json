{
  "name": "retoken-app",
  "version": "1.0.0",
  "description": "REToken App",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently -n 'server,client' -c 'blue,green' \"nodemon server.js\" \"npm run client\" ",
    "heroku-prebuild": "echo This runs before Heroku installs dependencies.",
    "heroku-postbuild": "npm install && cd client && npm install && npm run build",
    "heroku-cleanup": "echo This runs after Heroku prunes and caches dependencies.",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReToken-Co/retoken-app.git"
  },
  "author": "REToken Co",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "nodemon": "^2.0.7"
  },
  "bugs": {
    "url": "https://github.com/ReToken-Co/retoken-app/issues"
  },
  "homepage": "https://github.com/ReToken-Co/retoken-app#readme"
}
